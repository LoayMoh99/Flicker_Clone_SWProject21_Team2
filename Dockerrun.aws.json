{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "nginx",
        "image": "loaymoh99/dkr-demo-nginx",
        "hostname": "nginx",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "links": ["front", "api"],
        "memory": 100
      },
      {
        "name": "client",
        "image": "loaymoh99/dkr-demo-front",
        "hostname": "front",
        "essential": false,
        "memory": 200
      },
      {
        "name": "api",
        "image": "loaymoh99/dkr-demo-api",
        "hostname": "api",
        "essential": false,
        "memory": 200
      }
    ]
}